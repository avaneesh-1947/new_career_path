{
  "name": "career-guidance-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:fast": "next dev --turbo --port 3000",
    "build": "next build",
    "build:prod": "NEXT_CONFIG_FILE=next.config.prod.js next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next out dist",
    "dev:clean": "npm run clean && npm run dev"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "critters": "^0.0.23",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "next-i18next": "^15.2.0",
    "postcss": "^8.4.32",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.12.0",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.3.6"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "rimraf": "^6.0.1",
    "typescript": "^5"
  }
